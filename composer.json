{
  "name": "digital-creative/nova-inline-morph-to",
  "description": "A Laravel Nova field for displaying morphTo relationship inline.",
  "keywords": [
    "laravel",
    "nova",
    "field",
    "morph",
    "morphTo",
    "inline"
  ],
  "authors": [
    {
      "name": "Rafael Milewski"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/nova": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "DigitalCreative\\InlineMorphTo\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DigitalCreative\\InlineMorphTo\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
